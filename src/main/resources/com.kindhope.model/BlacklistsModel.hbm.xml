<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kindhope.model.BlacklistsModel" table="blacklists" schema="kindhope">
        <composite-id mapped="true" class="com.kindhope.model.BlacklistsModelPK">
            <key-property name="userId" column="user_id"/>
            <key-property name="blockedUserId" column="blocked_user_id"/>
        </composite-id>
        <property name="createdAt" column="created_at"/>
    </class>
</hibernate-mapping>