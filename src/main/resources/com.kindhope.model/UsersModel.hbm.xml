<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kindhope.model.UsersModel" table="users" schema="kindhope">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="email" column="email"/>
        <property name="password" column="password"/>
        <property name="rememberToken" column="remember_token"/>
        <property name="blockedAt" column="blocked_at"/>
        <property name="deletedAt" column="deleted_at"/>
        <property name="createdAt" column="created_at"/>
        <property name="updatedAt" column="updated_at"/>
        <property name="avatar" column="avatar"/>
    </class>
</hibernate-mapping>