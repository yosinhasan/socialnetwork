<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kindhope.entity.UsersEntity" table="users" schema="kindhope">
        <id name="id" column="id"/>
        <property name="name" column="name"/>
        <property name="email" column="email"/>
        <property name="password" column="password"/>
        <property name="rememberToken" column="remember_token"/>
        <property name="blockedAt" column="blocked_at"/>
        <property name="deletedAt" column="deleted_at"/>
        <property name="createdAt" column="created_at"/>
        <property name="updatedAt" column="updated_at"/>
        <set name="blacklistsesById" inverse="true">
            <key>
                <column name="user_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.BlacklistsEntity"/>
        </set>
        <set name="blacklistsesById_0" inverse="true">
            <key>
                <column name="blocked_user_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.BlacklistsEntity"/>
        </set>
        <set name="connectionRequestsesById" inverse="true">
            <key>
                <column name="user_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.ConnectionRequestsEntity"/>
        </set>
        <set name="connectionRequestsesById_0" inverse="true">
            <key>
                <column name="request_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.ConnectionRequestsEntity"/>
        </set>
        <set name="connectionsesById" inverse="true">
            <key>
                <column name="user_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.ConnectionsEntity"/>
        </set>
        <set name="connectionsesById_0" inverse="true">
            <key>
                <column name="friend_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.ConnectionsEntity"/>
        </set>
        <set name="galleriesById" inverse="true">
            <key>
                <column name="user_id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.GalleriesEntity"/>
        </set>
        <set name="photosesById" inverse="true">
            <key>
                <column name="user_id" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.kindhope.entity.PhotosEntity"/>
        </set>
    </class>
</hibernate-mapping>